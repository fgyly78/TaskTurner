<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--App Accent Dark-->
    <SolidColorBrush x:Key="AccentDark" Color="#FF164863" />
    <SolidColorBrush x:Key="AccentDarkOver" Color="#FF133E56" />
    <SolidColorBrush x:Key="AccentDarkClick" Color="#FF2A81B1" />
    <!--App OnAccent-->
    <SolidColorBrush x:Key="OnAccent" Color="#FFFFFFFF"/>
    <!--Task List Background-->
    <SolidColorBrush x:Key="TaskListBackground" Color="#FFECECEC" />
    <!--Task View Background-->
    <SolidColorBrush x:Key="WindowBackground" Color="#FFFFFFFF" />
    <!--Task Status Background-->
    <SolidColorBrush x:Key="TaskStatusBackground" Color="#FFF6F6F6" />
    <!--Default Text Color-->
    <SolidColorBrush x:Key="DefaultText" Color="#FF474747" />
    
    <!--AccnetForPolygon-->
    <SolidColorBrush x:Key="AccentDarkPolygon" Color="#FF2A566D" />
    
    <!--TextAccent-->
    <SolidColorBrush x:Key="TextAccent" Color="#FFF9EACE"/>

    <!--Borders-->
    <SolidColorBrush x:Key="Borders" Color="#FFCCCCCC"/>
    
    <!--Placeholder-->
    <SolidColorBrush x:Key="Placeholder" Color="#FFCCCCCC"/>

    <!--Buttons-->
    <SolidColorBrush x:Key="ButtonEditColor" Color="#FF9C9C9C"/>
    <SolidColorBrush x:Key="ButtonEditOver" Color="#FF737373"/>
    <SolidColorBrush x:Key="ButtonEditClick" Color="#FFC3C3C3"/>

    <SolidColorBrush x:Key="ButtonDeleteColor" Color="#FFC14A4A"/>
    <SolidColorBrush x:Key="ButtonDeleteOver" Color="#FFA22525"/>
    <SolidColorBrush x:Key="ButtonDeleteClick" Color="#FFF35757"/>

    <SolidColorBrush x:Key="ButtonCompleteColor" Color="#FF71B94A"/>
    <SolidColorBrush x:Key="ButtonCompleteOver" Color="#FF548E35"/>
    <SolidColorBrush x:Key="ButtonCompleteClick" Color="#FF94E67D"/>
    
    <!--Nav Buttons-->
    <SolidColorBrush x:Key="NavDefault" Color="Transparent"/>
    <SolidColorBrush x:Key="NavOver" Color="#FF133E56"/>
    <SolidColorBrush x:Key="NavPressed" Color="#FF2A81B1"/>

    <!--ImportanceColors-->

    <!--Low Importance-->
    <SolidColorBrush x:Key="Importance-Low" Color="#FF0CAFF4" />
    
    <!--Medium Importance-->
    <SolidColorBrush x:Key="Importance-Med" Color="#FF2AD985" />
    
    <!--High Importance-->
    <SolidColorBrush x:Key="Importance-high" Color="#FFCA2C2C"/>
    
    <!--Critical-->
    <SolidColorBrush x:Key="Importance-Crit" Color="#FFF4B30C" />

    <!--Category-->
    <SolidColorBrush x:Key="Importance-Category" Color="#FF6646AF" />

    <!--Task Progress-->
    <SolidColorBrush x:Key="Progress-InProgress" Color="#FF518657" />
    <SolidColorBrush x:Key="Progress-Completed" Color="#FF4187AC" />
    <SolidColorBrush x:Key="Progress-NotStarted" Color="#FFB1B1B1" />
    <SolidColorBrush x:Key="Progress-Late" Color="#FFB46161" />
    <SolidColorBrush x:Key="Progress-Archieved" Color="#FFC8A36C" /> 
    <SolidColorBrush x:Key="Progress-Deleted" Color="#FF606060"/>
</ResourceDictionary>